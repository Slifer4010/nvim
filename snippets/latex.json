{
  "Template": {
    "prefix": ["template", "\\template"],
    "body": [
      "\\documentclass{article}",
      "\\usepackage[utf8]{inputenc}",
      "\\usepackage[spanish]{babel}",
      "\\usepackage{amsmath}",
      "\\usepackage{amssymb}",
      "\\usepackage{cite}",
      "\\usepackage{graphicx}",
      "\\usepackage{float}",
      "\\usepackage{caption}",
      "\\usepackage{fancyhdr}",
      "\\usepackage[colorlinks=true,linkcolor=black,urlcolor=black]{hyperref}"
    ]
  },
  "Fracción": {
    "prefix": ["frac"],
    "body": ["\\frac{$1}{$2}"]
  },
  "Item": {
    "prefix": ["item"],
    "body": ["\\item $1"]
  },
  "Enviroment": {
    "prefix": ["begin"],
    "body": ["\\begin{${1:Enviroment}}", "  ${2}", "\\end{${1:Enviroment}}"]
  },
  "Figure": {
    "prefix": ["figure"],
    "body": [
      "\\begin{figure}[h]",
      "  \\begin{center}",
      "    \\fbox{\\includegraphics[scale=0.4]{${1:Figure}}}",
      "      \\caption{${2:Caption}}",
      "      \\label{${3:Label}}",
      "  \\end{center}",
      "\\end{figure}"
    ]
  },
  "Figura": {
    "prefix": ["Figura"],
    "body": [
      "Figura \\ref{${3:Label}} $0",
      "",
      "\\begin{figure}[h]",
      "  \\begin{center}",
      "    \\fbox{\\includegraphics[scale=0.4]{${1:Figure}}}",
      "      \\caption{${2:Caption}}",
      "      \\label{${3:Label}}",
      "  \\end{center}",
      "\\end{figure}"
    ]
  },
  "Equation": {
    "prefix": ["equation"],
    "body": [
      "\\begin{equation}",
      "  ${2:equation}",
      "  \\label{${1:Label}}",
      "\\end{equation}"
    ]
  },
  "Picture": {
    "prefix": ["picture"],
    "body": [
      "\\begin{figure}[h]",
      "  \\begin{center}",
      "    \\fbox{",
      "      \\begin{tikzpicture}",
      "        ${3:picture}",
      "      \\end{tikzpicture}",
      "      \\caption{${1:Caption}}",
      "      \\label{${2:Label}}",
      "    }",
      "  \\end{center}",
      "\\end{figure}"
    ]
  },
  "Code": {
    "prefix": ["coding"],
    "body": [
      "\\usepackage{listings}",
      "\\usepackage{xcolor}",
      "",
      "\\definecolor{backcolor}{RGB}{250,250,250}",
      "\\definecolor{keyword}{RGB}{166,38,164}",
      "\\definecolor{numbers}{RGB}{1,132,188}",
      "\\definecolor{string}{RGB}{80,161,79}",
      "\\definecolor{comment}{RGB}{170,170,170}",
      "\\definecolor{codeblack}{RGB}{15,16,17}",
      "",
      "\\lstdefinestyle{code}{",
      "  backgroundcolor=\\color{backcolor},",
      "  commentstyle=\\color{comment},",
      "  keywordstyle=\\color{keyword},",
      "  numberstyle=\\tiny\\color{numbers},",
      "  stringstyle=\\color{string},",
      "  basicstyle=\\ttfamily\\footnotesize\\color{codeblack},",
      "  breakatwhitespace=false,",
      "  breaklines=true,",
      "  captionpos=b,",
      "  keepspaces=true,",
      "  numbers=left,",
      "  numbersep=0.2cm,",
      "  showspaces=false,",
      "  showstringspaces=false,",
      "  showtabs=false,",
      "  tabsize=2,",
      "}"
    ]
  },

  "MatLab code": {
    "prefix": ["mcode"],
    "body": [
      "\\lstinputlisting[style=Matlab-Pyglike, frame=single, numbers=left, captionpos=b,caption={$1}, label={Cod:$2}]{$3}$4"
    ]
  },
  "Tabla de verdad": {
    "prefix": "tdv",
    "body": [
      "\\textbf{Tabla de verdad de:} $1  \\newline",
      "\\textbf{Número de interpretaciones:} \\newline",
      "\\begin{table}[H]",
      "\\centering",
      "\\begin{tabular}{}",
      "\\hline",
      "\\rowcolor{lightgray}",
      "\\hline",
      "\\end{tabular}",
      "\\end{table}"
    ]
  },
  "Tabla de verdad final": {
    "prefix": "tdvf",
    "body": [
      "\\textbf{Tabla de verdad de:} $1  \\newline",
      "\\textbf{Número de interpretaciones:} \\newline",
      "\\begin{table}[H]",
      "\\centering",
      "\\begin{tabular}{}",
      "\\hline",
      "\\rowcolor{lightgray}",
      "\\hline",
      "\\end{tabular}",
      "\\end{table}",
      "\\textbf{Usando el método de substitución de equivalencias:}",
      "\\begin{align*}",
      "\\end{align*}",
      "\\textbf{Respuesta:$$}"
    ]
  },
  "Doble implicacion": {
    "prefix": "bimpli",
    "body": "\\leftrightarrow "
  },
  "Implicacion": {
    "prefix": "impli",
    "body": "\\rightarrow "
  },
  "Respuesta": {
    "prefix": "tr",
    "body": "\\text{$1} \\equiv & $2"
  },
  "Respuesta Formal": {
    "prefix": "trf",
    "body": "\\text{$1} \\equiv & $2 \\longrightarrow \\text{\\textcolor{blue}{$3}}"
  },
  "Premisa inicial": {
    "prefix": "pri",
    "body": "$1) & \\quad $2 && \\text{Premisa inicial} "
  },
  "Premisa inicial final": {
    "prefix": "prif",
    "body": ["$1) & \\quad $2 && \\text{Premisa inicial} $3", "\\cline{2-4}"]
  },
  "Premisa inicial subdemostracion": {
    "prefix": "spri",
    "body": "$1)\\quad $2 &&& \\text{Premisa inicial} $3"
  },
  "Premisa inicial final subdemostracion": {
    "prefix": "sprif",
    "body": ["$1)\\quad $2 &&& \\text{Premisa inicial} $3", "\\cline{1-4}"]
  },
  "Sub refutacion": {
    "prefix": "srf",
    "body": "\\hspace{1cm} $1) & \\quad $2 && \\text{$3} "
  },
  "Refutacion con sub": {
    "prefix": "rfs",
    "body": "$1)\\quad $2 &&& \\text{$3} "
  },
  "Refutacion con sub final": {
    "prefix": "rfsf",
    "body": [
      "$1)\\quad $2 &&& \\text{$3} $4",
      "$5)\\phantom{$2} &&& \\text{$6}",
      "&&& \\text{Q.E.D.}"
    ]
  },
  "Refutacion": {
    "prefix": "rf",
    "body": "$1) & \\quad $2 && \\text{$3} "
  },
  "Demostración": {
    "prefix": "dt",
    "body": "\\textbf{Técnica de demostración: $1} $2"
  },
  "Refutacion final": {
    "prefix": "rff",
    "body": ["$1) & \\quad $2 && \\text{$3} $4", " & && \\text{Q.E.D.}"]
  },
  "Consecuente negado": {
    "prefix": "cn",
    "body": [
      "$1) & \\quad $2 && \\text{Negación del consecuente original} $4",
      "\\cline{2-4}"
    ]
  },
  "Relacion": {
    "prefix": "rel",
    "body": "R=\\\\{(x,y)| $1 \\\\}"
  },
  "Relacion conjunto": {
    "prefix": "relc",
    "body": "$1=\\\\{($2),$3 \\\\}"
  },
  "Conjunto": {
    "prefix": "conj",
    "body": "($1),"
  },
  "Conectar": {
    "prefix": "conec",
    "body": "\\draw ($1) -- ($2);"
  },
  "Align": {
    "prefix": "align",
    "body": ["\\begin{align*}", "$1", "\\end{align*}"]
  },
  "Digrafo": {
    "prefix": "grafo",
    "body": [
      "\\begin{figure}[ht]",
      "\\centering",
      "\\tikzset{smallstate/.style={state, inner sep=1pt, minimum size=6mm}}",
      "\\begin{tikzpicture}[->,>=stealth', shorten >=1pt, auto, node distance=4cm,]",
      "$1",
      "\\end{tikzpicture}",
      "\\end{figure}"
    ]
  },
  "Nodo": {
    "prefix": "nodo",
    "body": ["\\node[smallstate] ($1) {$2};"]
  },
  "Nodo abajo": {
    "prefix": "nodoabajo",
    "body": ["\\node[smallstate, below=0.5cm of $1] ($2) {$3};"]
  },
  "Nodo derecho": {
    "prefix": "nododerecho",
    "body": ["\\node[smallstate, right=of $1] ($2) {$3};"]
  }
}
